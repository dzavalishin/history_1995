  '°', '±', '²', '³', '΄', 'µ', '¶', '·',	// 128
  'Έ', 'Ή', 'Ί', '»', 'Ό', '½', 'Ύ', 'Ώ',	// 136
  'ΐ', 'Α', 'Β', 'Γ', 'Δ', 'Ε', 'Ζ', 'Η',	// 144
  'Θ', 'Ι', 'Κ', '?', 'Μ', 'Ν', 'Ξ', 'Ο',	// 152

  'Π', 'Ρ', '', 'ς', 'Τ', 'Υ', 'Φ', 'Χ',	// 160
  'Ψ', 'Ω', 'Ϊ', 'Ϋ', 'ά', 'έ', 'ή', 'ί',	// 168
  'Σ', 'σ', 'ς', 'ρ', 'τ', 'υ', 'φ', 'χ',	// 176
  'ψ', 'ω', 'ϊ', 'ϋ', 'ό', 'ύ', 'ώ', 'Λ',	// 184

  0xee, 0xa0, 0xa1, 0xe6, 0xa4, 0xa5, 0xe4, 0xa3,	// 192
  0xe5, 0xa8, 0xa9, 0xaa, 0xab, 0xac, 0xad, 0xae,	// 200
  0xaf, 0xef, 0xe0, 0xe1, 0xe2, 0xe3, 0xa6, 0xa2,	// 208
  0xec, 0xeb, 0xa7, 0xe8, 0xed, 0xe9, 0xe7, 0xea,	// 216

